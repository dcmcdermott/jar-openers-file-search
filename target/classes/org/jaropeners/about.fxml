<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.jaropeners.AboutController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox alignment="TOP_CENTER" prefHeight="388.0" prefWidth="636.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" >
        <HBox prefHeight="100.0" prefWidth="200.0">
            <ImageView fitHeight="100.0" fitWidth="113.0" pickOnBounds="true" preserveRatio="true" />
            <Label contentDisplay="CENTER" prefHeight="102.0" prefWidth="164.0" text="About" textAlignment="JUSTIFY" textOverrun="CENTER_ELLIPSIS">
                <font>
                    <Font size="54.0" />
                </font>
                <HBox.margin>
                    <Insets left="260.0" />
                </HBox.margin>
            </Label>
        </HBox>
        <TilePane alignment="CENTER_LEFT" prefHeight="242.0" prefWidth="636.0">
            <Label contentDisplay="CENTER" prefHeight="30.0" prefWidth="254.0" text=" Jar Openers File Search 1.0">
                <TilePane.margin>
                    <Insets left="320.0" />
                </TilePane.margin>
                <font>
                    <Font name="System Bold" size="19.0" />
                </font>
            </Label>
            <Label prefHeight="25.0" prefWidth="93.0" text="Contributors:">
                <TilePane.margin>
                    <Insets left="180.0" />
                </TilePane.margin>
                <font>
                    <Font size="15.0" />
                </font>
            </Label>
            <Label prefHeight="17.0" prefWidth="99.0" text="Devin McDermott">
                <TilePane.margin>
                    <Insets left="196.0" />
                </TilePane.margin>
            </Label>
            <Label text="Amanda Camelio">
                <TilePane.margin>
                    <Insets left="188.0" />
                </TilePane.margin>
            </Label>
            <Label prefHeight="17.0" prefWidth="76.0" text="Justin Meek">
                <TilePane.margin>
                    <Insets left="176.0" />
                </TilePane.margin>
            </Label>
            <Label text="Joaquin Rojas chang">
                <TilePane.margin>
                    <Insets left="205.0" />
                </TilePane.margin>
            </Label>
        </TilePane>
        <HBox alignment="CENTER" prefHeight="44.0" prefWidth="636.0" spacing="100.0">
            <Button fx:id="btnGoBack" mnemonicParsing="false" style="-fx-background-color: #b3e5fc; -fx-border-color: #0288d1; -fx-border-radius: 3%; -fx-cursor: hand;" onAction="#switchToPrimary" text="Go Back" />
        </HBox>
    </VBox>
</AnchorPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="TOP_CENTER" prefHeight="388.0" prefWidth="636.0" spacing="20.0" style="-fx-background-color: #edf0f2;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jaropeners.SecondaryController">
        <Label prefHeight="45.0" prefWidth="273.0" style="-fx-font-weight: bold;" text="Index Maintenance">
         <font>
            <Font size="30.0" />
         </font></Label>
      <TableView fx:id="tvIndexedFiles" prefHeight="400.0" prefWidth="596.0">
        <columns>
          <TableColumn fx:id="col_filepath" prefWidth="298.0" text="File Name" />
          <TableColumn fx:id="col_last_modified" prefWidth="298.0" text="Last Modified" />
        </columns>
      </TableView>
      <HBox alignment="CENTER" spacing="100.0">
            <Button fx:id="btnAddFile" onAction="#addFile" mnemonicParsing="false" style="-fx-background-color: #b3e5fc; -fx-border-color: #0288d1; -fx-border-radius: 3%; -fx-cursor: hand;" text="Add File" />
            <Button fx:id="btnRemoveFile" onAction="#removeFile" mnemonicParsing="false" style="-fx-background-color: #b3e5fc; -fx-border-color: #0288d1; -fx-border-radius: 3%; -fx-cursor: hand;" text="Remove Selected File" />
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="500.0">
              <Button fx:id="btnBackToFileSearch" onAction="#switchToPrimary" prefHeight="27.0" prefWidth="122.0" style="-fx-background-color: #b3e5fc; -fx-border-color: #0288d1; -fx-border-radius: 3%; -fx-cursor: hand;" text="Back to File Search" textAlignment="CENTER" />
            <Label prefHeight="18.0" prefWidth="203.0" text="Jar Openers File Search Version 1.0" />
         </HBox>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
